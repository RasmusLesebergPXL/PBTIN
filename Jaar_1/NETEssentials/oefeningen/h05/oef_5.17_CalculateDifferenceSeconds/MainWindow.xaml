        <Label Content="Tijd 2: Minuten" HorizontalAlignment="Left" Height="34" Margin="86,249,0,0" VerticalAlignment="Top" Width="112"/>
        <Label Content="Tijd 2: Seconden" HorizontalAlignment="Left" Height="35" Margin="86,283,0,0" VerticalAlignment="Top" Width="112" RenderTransformOrigin="0.494,0.201"/>
